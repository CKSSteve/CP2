#table-count{
	padding:3px;
}
#table-count td{
	font-family:sans-serif, Geneva,Arial,Helvetica,"新細明體","細明體",sans-serif;
	font-size: 9pt;
	color:#FF4500;
	line-height: 150%;
	border:0px;
}
#table_none{
//	font-family:sans-serif, Geneva,Arial,Helvetica;
	color:#333333;
	border:0px;
}

#table_none a:link,#table_none a:visited,#table_none a:active{
	color:#636563;
	line-height:150%;
	text-decoration: none;
	margin:2px 0px 2px 0px;
}
#table_none a:hover{
	color:#0066CC;
	text-decoration: underline;
}

#table_none td{
	line-height:150%;
	text-align:left;
	font-size: 9pt;
	border-width:0px;
}

#table_10pt{
	font-family:sans-serif, Geneva,Arial,Helvetica,"新細明體","細明體",sans-serif;
	font-size: 10pt;
	color:#333333;
	line-height: 200%;
	border:0px;
}
.td-lighty{
	background-color:#FFEFD5;
}
.txt11-red{
	font-family:sans-serif, Geneva,Arial,Helvetica,"新細明體","細明體",sans-serif;
	color:#8B4513;
	font-size: 11pt;
	font-weight:bold;
	text-align:center;
	border-bottom:2px solid #D2691E;
	padding:3px 3px 5px 3px;
	letter-spacing:10px;
}
#table_header{
	font-family:sans-serif, Geneva,Arial,Helvetica,"新細明體","細明體",sans-serif;
	color:#696969;
	border:0px;
	font-size:10pt;
}
#table_header a:link,#table_header a:visited,#table_header a:active{
	color:#8B4513;
	line-height:150%;
	text-decoration: none;
	margin:2px 0px 2px 0px;
}
#table_header a:hover{
	color:#800000;
	text-decoration: underline;
}
#table_header td{
	color:#000000;
	line-height:150%;
	text-align:left;
	font-size: 9pt;
	border-width:0px;
	vertical-align:top;
}




#table_print{
	color:#333333;
	border:0px;
}

#table_print td{
	line-height:150%;
	text-align:left;
	font-size: 14pt;
	border-width:0px;
}


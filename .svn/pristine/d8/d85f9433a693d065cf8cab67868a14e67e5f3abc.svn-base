/*--完成訊息視窗--*/
#table_msg{
	font-family:sans-serif, Geneva,Arial,Helvetica,"新細明體","細明體",sans-serif;
	color:#0A4B96;/*深藍色*/
}
#table_msg th{
	padding:3px 3px 3px 3px;
	text-align:center;
	font-size:11pt;
	background:#50CCB1;
	font-weight:normal;
	letter-spacing:5px;
	color:#FFFFFF;
}
#table_msg td{
	padding:2px 3px 3px 3px;
	text-align:center;
	font-size:11pt;
}
/*--明細列視窗--*/
#table01{
	font-family:sans-serif, Geneva,Arial,Helvetica,"新細明體","細明體",sans-serif;
	border-left:1px solid #DEB887;
	border-bottom:2px solid #50CCB1;
	margin:0px 0px 0px 0px;
	color:#333333;
}
#table01 th {
	padding:3px 3px 3px 3px;
	border-right:1px solid #DEB887;
	font-size: 12px;
	font-weight:normal;
	line-height: 160%;
	color: #FFFFFF;
	background-color:#50CCB1;
}
#table01 td{
	padding:3px;
	border-right:1px solid #DEB887;
	border-bottom:1px solid #DEB887;
	font-size: 12px;
	line-height: 160%;
	text-align:center;
}

#table02{
	font-size:13px;
	border-top:1px solid #D3D3D3;
	border-left:1px solid #D3D3D3;
	margin:0px 0px 0px 0px;
	color:#333333;
	border-bottom:3px solid #50CCB1;
}
#table02_noborder{
	margin:1px 0px 0px 0px;
	color:#333333;
	font-size:14px;
	border-bottom:3px solid #50CCB1;
	line-height: 180%;
}
#table02 th {
	padding:2px 0px 3px 0px;
	font-family:sans-serif,Geneva,Arial,Helvetica,"新細明體","細明體",sans-serif;
	font-size: 12px;
	color: #FFFFFF;
	background-color:#3C8AD8;
	font-weight:normal;
	border-right:1px solid #D3D3D3;
	border-bottom:1px solid #D3D3D3;
	line-height: 20px;
}
#table02 td{
	font-size: 13px;
	line-height: 180%;
	text-align:left;
	border-right:1px solid #D3D3D3;
	border-bottom:1px solid #D3D3D3;
	padding:3px;
}
#table02_noborder h2{
 color:#333333;
 font-size:14pt;
}
#table03{
	border-top:1px solid #333333;
	border-left:1px solid #333333;
	margin:1px 0px 0px 0px;
	color:#333333;
}
#table03 th {
	padding:2px 0px 3px 0px;
	font-family:sans-serif,Geneva,Arial,Helvetica,"新細明體","細明體",sans-serif;
	font-size: 12px;
	color: #FFFFFF;
	background-color:#3C8AD8;
	font-weight:normal;
	border-right:1px solid #D3D3D3;
	border-bottom:1px solid #D3D3D3;
	line-height: 20px;
}
#table03 td{
	font-family:Geneva,Arial,Helvetica,"新細明體","細明體",sans-serif;
	font-size: 12px;
	line-height: 180%;
	text-align:center;
	border-right:1px solid #333333;
	border-bottom:1px solid #333333;
	padding:4px;
}
#table04 td{
	color:#333333;
	font-size: 12px;
	padding:4px;
	line-height: 180%;
}
#table05{
	border-left:1px solid #DEB887;
	margin:1px 0px 0px 0px;
	color:#333333;
	border-top:2px solid #50CCB1;
	border-bottom:2px solid #50CCB1;
}
#table05 th {
	padding:2px 5px 3px 10px;
	font-family:sans-serif,Geneva,Arial,Helvetica,"新細明體","細明體",sans-serif;
	font-size: 12px;
	color: #FFFFFF;
	background-color:#50CCB1;
	font-weight:normal;
	border-right:1px solid #DEB887;
	border-bottom:1px solid #DEB887;
	line-height: 20px;
}
#table05 td{
	font-family:Geneva,Arial,Helvetica,"新細明體","細明體",sans-serif;
	font-size: 12px;
	line-height: 180%;
	text-align:center;
	border-right:2px solid #50CCB1;
	border-bottom:1px solid #DEB887;
	padding:4px;
}


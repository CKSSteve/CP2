.left-menu-002{
	font-family:sans-serif,Geneva,Arial,Helvetica,"新細明體","細明體";
	font-size:12px;
	color:#FFFFFF;
	line-height:150%;
	margin:0px;
	border-bottom:0px solid #FDC52D;
}
.left-menu-002 a:link,.left-menu-002 a:visited,.left-menu-002 a:active {
	display:block;
	color:#000000;
	background: url(../images/left_menu_image/menu_010_up.jpg) repeat-y;
	padding: 0px 0px 5px 18px;
	vertical-align: middle;
	text-decoration: none;
}
.left-menu-002 a:hover {
	font-size: 12px;
	color: #3F95EA;
	text-decoration: none;
	background: url(../images/left_menu_image/menu_010_up.jpg) repeat-y;
}
.left-menu-003{

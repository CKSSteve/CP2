package com.uxb2b.ecp.bean.swift;


/*
 * 查詢委請書、聲明書資料(Q05)上行電文
 * */
public class Q05RqBean {

	private String TX_ID;
	private String TX_TIME;
	private String BATCH_NO;
	
	public Q05RqBean(){}
	
	public Q05RqBean(String TX_ID,String TX_TIME,String BATCH_NO){
		this.TX_ID=TX_ID;
		this.TX_TIME=TX_TIME;
		this.BATCH_NO=BATCH_NO;
	}
	
	public String getTX_ID() {
		return TX_ID;
	}
	public void setTX_ID(String tX_ID) {
		TX_ID = tX_ID;
	}
	public String getTX_TIME() {
		return TX_TIME;
	}
	public void setTX_TIME(String tX_TIME) {
		TX_TIME = tX_TIME;
	}
	public String getBATCH_NO() {
		return BATCH_NO;
	}
	public void setBATCH_NO(String bATCH_NO) {
		BATCH_NO = bATCH_NO;
	}
	
	
}
